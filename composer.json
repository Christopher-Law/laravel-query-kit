{
    "name": "makeabledk/laravel-querykit",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "Makeable\\QueryKit\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^5.4|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5|^4.0",
        "friendsofphp/php-cs-fixer": "^2.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Makeable\\QueryKit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Makeable\\QueryKit\\QueryKitServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "style": "./vendor/bin/php-cs-fixer fix ./src --rules=@PSR2,@Symfony,ordered_class_elements,ordered_imports --verbose"
    }
}
